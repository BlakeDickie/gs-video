<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.0//EN" "http://www.netbeans.org/dtds/filesystem-1_0.dtd">
<filesystem>
 
    <folder name="GSVideoManager">
        <file name="RenameScript.py" url="RenameScript.py"/>
        <file name="RenameScript_File.py" url="RenameScript_File.py"/>
        <file name="RenameScript_Folder.py" url="RenameScript_Folder.py"/>
    </folder>
    
    <folder name="OptionsDialog">
        <file name="FileRenaming.instance">
            <attr name="instanceCreate" methodvalue="org.netbeans.spi.options.OptionsCategory.createCategory"/>
            <attr name="title" bundlevalue="net.landora.videofilerenaming.Bundle#OptionsCategory_Name_FileRenaming"/>
            <attr name="categoryName" bundlevalue="net.landora.videofilerenaming.Bundle#OptionsCategory_Name_FileRenaming"/>
            <attr name="iconBase" stringvalue="net/landora/videofilerenaming/edit-find-replace.png"/>
            <attr name="advancedOptionsFolder" stringvalue="OptionsDialog/FileRenaming"/>
            <attr name="keywords" bundlevalue="net.landora.videofilerenaming.Bundle#OptionsCategory_Keywords_FileRenaming"/>
            <attr name="keywordsCategory" stringvalue="FileRenaming"/>
            <attr name="description" bundlevalue="net.landora.videofilerenaming.Bundle#OptionsCategory_Name_FileRenaming"/>
            <attr name="position" intvalue="90"/>
        </file>
    </folder>
</filesystem>
